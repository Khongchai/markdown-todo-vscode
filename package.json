{
  "name": "markdown-todo-vscode",
  "private": true,
  "displayName": "Markdown Dated Todo List",
  "version": "0.0.5",
  "publisher": "Khongchai",
  "repository": {
    "type": "git",
    "url": "https://github.com/Khongchai/markdown-todo-vscode.git"
  },
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "contributes": {
    "grammars": [
      {
        "injectTo": [
          "text.html.markdown"
        ],
        "scopeName": "markdown-todo",
        "path": "./syntaxes/injection.json"
      }
    ]
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\"",
    "watch": "tsc -watch -p ./",
    "test": "jest",
    "test-coverage": "jest --coverage"
  },
  "devDependencies": {
    "@swc/core": "^1.3.71",
    "@swc/jest": "^0.2.27",
    "@types/jest": "^29.5.2",
    "@types/node": "^16.18.34",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "jest": "^29.6.2",
    "typescript": "^5.1.3"
  }
}
